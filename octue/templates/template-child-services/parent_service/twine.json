		"type": "object",
		"properties": {
			"wind_speeds": {
				"description": "The wind speed at the location.",
				"type": "array",
				"items": {
					"type": "number",
					"minimum": 0
				}
			},
			"elevations": {
				"description": "The elevation at the location.",
				"type": "array",
				"items": {
