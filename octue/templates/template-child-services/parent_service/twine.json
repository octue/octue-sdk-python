			"filters": "labels:elevation"
		}
	],
	"input_values_schema": {
		"$schema": "http://json-schema.org/2019-09/schema#",
		"title": "Input values",
		"description": "Locations as longitude-latitude pairs",
		"type": "object",
		"properties": {
			"locations": {
				"type": "array",
				"items": [
					{
						"type": "object",
						"properties": {
							"longitude": {
